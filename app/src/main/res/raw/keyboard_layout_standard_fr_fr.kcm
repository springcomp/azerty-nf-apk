type OVERLAY

key 1 {
  number:                     '1'
  label:                      '1'
  base:                       '\u00e0' # 'à' LATIN SMALL LETTER A WITH GRAVE
  shift, capslock:            '1'
  ralt, ctrl+alt:             '\u00a7' # '§' SECTION SIGN
  shift+ralt, shift+ctrl+alt: '\u00c0' # 'À' LATIN CAPITAL LETTER A WITH GRAVE
  ctrl, alt, meta:             none
}

